# BPR&D API Configuration
# Copy this file to .env and fill in your actual keys.
# DO NOT commit your .env file to version control.

# --- AI Platform Keys ---

# Abacus.AI (Deep Agent / ChatLLM)
ABACUS_PRIMARY_KEY=your_abacus_primary_key_here
ABACUS_BACKUP_KEY=your_abacus_backup_key_here

# xAI (Grok)
GROK_API_KEY=your_grok_api_key_here

# Anthropic (Claude)
CLAUDE_API_KEY=your_claude_api_key_here

# Google (Gemini)
GEMINI_API_KEY=your_gemini_api_key_here
# Legacy Codebuff/Jules Key
GEMINI_LEGACY_KEY=your_gemini_legacy_key_here

# Render
RENDER_API_KEY=your_render_api_key_here

# Google Cloud (Path to JSON key file)
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/google-credentials.json

